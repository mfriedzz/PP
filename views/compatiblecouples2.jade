
extends layout
block content
    .container-fluid
      //- // /* Navigation Bar */
      //- // <nav class="navbar navbar-inverse" role="navigation">
      nav.navbar.navbar-inverse.navbar-fixed-top.navbar-header(role='navigation')
        .container-fluid
          //- // <label id=projectname>Project Name</label>
          form.navbar-form.navbar-left
            .form-group
              a.navbar-brand(href='#')
                img#Logo(alt='Brand', src='logo/purplepeople2.gif')
                a.form-control(href='login2.html') Login
                a.form-control(href='/about.html') About
                a.form-control(href='compatiblecouples.html') Compatible Couples
                input.form-control(type='text', placeholder='Email')
                input.form-control(type='text', placeholder='Password')
                button.btn.btn-success(type='submit') Sign In
      //- // </div>  /* end row */
      .row
        .col-md-6.coupleprofile
          aside
            h2.profile Your Couple's Profile
            h3.profilestatistics Profile Statistics:
            h3  Your Couple Pictures: 
        //- // /* End of div class coupleprofile */
        .col-md-6.couplessearch
          h2(style='color:#000000') Compatible Couples Search Criteria:
          //- // Search Form
          form.search
            label
              | Age Range Min Age: 
              input#minage(type='number', name='minage', value='18', min='18', max='115')
              br
              |               Age Range Max Age: 
              input#maxage(type='number', name='maxage', value='18', min='18', max='115')
            br
            |             Children:
            input#childrenyes(type='radio', name='children', value='true')
            |  Yes
            input#childrenno(type='radio', name='children', value='false', checked='checked')
            |  No
            br
            |             Pets:
            input#petsyes(type='radio', name='pets', value='true')
            |  Yes
            input#petsno(type='radio', name='pets', value='false', checked='checked')
            |  No
            br
            |             Served in Military:
            input#militaryyes(type='radio', name='militaryservice', value='true')
            |  Yes
            input#militaryno(type='radio', name='militaryservice', value='false', checked='checked')
            |  No
            br
            br
            input#submitbutton(type='submit', name='submitform', value='Search')
          h2(style='color:#000000')  Couple Search Results 
          .couplesearchresults
        //- // /* end of couplessearch */
    //-   // /* end of Row class */
    //- // /* Container End */
    footer
